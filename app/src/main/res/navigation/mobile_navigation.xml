<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.myapplication.students.ui.home.HomeFragment"
        android:label="Home"
        tools:layout="@layout/fragment_home"/>

    <fragment
        android:id="@+id/navigation_notice"
        android:name="com.example.myapplication.students.ui.notice.NoticeFragment"
        android:label="Notice"
        tools:layout="@layout/fragment_notice"/>

    <fragment
        android:id="@+id/navigation_event"
        android:name="com.example.myapplication.students.ui.event.EventFragment"
        android:label="Event"
        tools:layout="@layout/fragment_event"/>

    <fragment
        android:id="@+id/navigation_gallery"
        android:name="com.example.myapplication.students.ui.gallery.GalleryFragment"
        android:label="Gallery"
        tools:layout="@layout/fragment_gallery"/>

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.example.myapplication.students.ui.profile.ProfileFragment"
        android:label="Profile"
        tools:layout="@layout/fragment_profile"/>

</navigation>